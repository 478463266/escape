<virtualizer>
    <id>DOCKER2</id>
    <name>Docker domain2</name>
    <nodes>
        <node>
            <id>DOCKER_HOST</id>
            <name>ee-infra-2</name>
            <type>BiSBiS</type>
            <ports>
                <port>
                    <id>1</id>
                    <name>SAP21</name>
                    <port_type>port-sap</port_type>
                    <sap>SAP21</sap>
                </port>
            </ports>
            <resources>
                <cpu>5</cpu>
                <mem>5</mem>
                <storage>20</storage>
            </resources>
            <links>
                <link>
                    <id>resource-link</id>
                    <src>../../../ports/port[id=1]</src>
                    <dst>../../../ports/port[id=1]</dst>
                    <resources>
                        <delay>0.9</delay>
                        <bandwidth>5000.0</bandwidth>
                    </resources>
                </link>
            </links>
            <capabilities>
                <supported_NFs>
                    <node>
                        <id>headerCompressor</id>
                        <type>headerCompressor</type>
                    </node>
                    <node>
                        <id>headerDecompressor</id>
                        <type>headerDecompressor</type>
                    </node>
                    <node>
                        <id>simpleForwarder</id>
                        <type>simpleForwarder</type>
                    </node>
                </supported_NFs>
            </capabilities>
        </node>
    </nodes>
</virtualizer>
