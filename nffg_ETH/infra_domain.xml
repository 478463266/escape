<?xml version="1.0" ?>
<virtualizer>
    <name>SG-name-ros-mapped</name>
    <nodes>
        <node>
            <NF_instances>
                <node>
                    <name>webserver</name>
                    <ports>
                        <port>
                            <name>port0</name>
                            <port_type>port-abstract</port_type>
                            <id>0</id>
                        </port>
                    </ports>
                    <type>webserver</type>
                    <id>webserver</id>
                    <resources>
                        <mem>1</mem>
                        <cpu>1</cpu>
                    </resources>
                </node>
            </NF_instances>
            <flowtable>
                <flowentry>
                    <port>../../../ports/port[id=0]</port>
                    <priority>100</priority>
                    <action>strip_vlan</action>
                    <id>0</id>
                    <match>dl_vlan=0x0001</match>
                    <out>
                        ../../../NF_instances/node[id=webserver]/ports/port[id=0]
                    </out>
                </flowentry>
                <flowentry>
                    <port>
                        ../../../NF_instances/node[id=webserver]/ports/port[id=0]
                    </port>
                    <priority>100</priority>
                    <action>mod_vlan_vid:0x0002</action>
                    <id>1</id>
                    <out>../../../ports/port[id=1]</out>
                </flowentry>
            </flowtable>
            <capabilities>
                <supported_NFs>
                    <node>
                        <name>echo</name>
                        <ports>
                            <port>
                                <name>input port</name>
                                <port_type>port-abstract</port_type>
                                <id>0</id>
                            </port>
                        </ports>
                        <type>echo</type>
                        <id>NFecho</id>
                    </node>
                    <node>
                        <name>webserver</name>
                        <ports>
                            <port>
                                <name>input port</name>
                                <port_type>port-abstract</port_type>
                                <id>0</id>
                            </port>
                        </ports>
                        <type>webserver</type>
                        <id>NFwebserver</id>
                    </node>
                </supported_NFs>
            </capabilities>
            <ports>
                <port>
                    <name>SAP24</name>
                    <port_type>port-sap</port_type>
                    <sap>SAP24</sap>
                    <id>0</id>
                </port>
                <port>
                    <name>SAP42</name>
                    <port_type>port-sap</port_type>
                    <sap>SAP42</sap>
                    <id>1</id>
                </port>
            </ports>
            <type>BisBis</type>
            <id>UUID-01</id>
            <resources>
                <mem>32</mem>
                <storage>5</storage>
                <cpu>10</cpu>
            </resources>
            <name>single Bis-Bis node representing the whole OS domain</name>
        </node>
    </nodes>
    <id>OS-req</id>
</virtualizer>