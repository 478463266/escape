{
  "service": {
    "MAPPER": {
      "mapping-enabled": false
    }
  },
  "orchestration": {
    "Sl-Or": {
      "virtualizer_type": "SINGLE"
    },
    "ESCAPE-SERVICE": {
      "virtualizer_type": "SINGLE"
    }
  },
  "adaptation": {
    "CLEAR-DOMAINS-AFTER-SHUTDOWN": true,
    "MANAGERS": [
      "MININET"
    ],
    "MININET": {
      "module": "escape.adapt.managers",
      "class": "UnifyDomainManager",
      "domain_name": "MININET",
      "diff": true,
      "poll": false,
      "adapters": {
        "REMOTE": {
          "module": "escape.adapt.adapters",
          "class": "UnifyRESTAdapter",
          "prefix": "ro/mininet",
          "url": "http://127.0.0.1:8899"
        },
        "CALLBACK": {
          "enabled": false,
          "address": "0.0.0.0",
          "port": 9000,
          "explicit_update": true
        }
      }
    },
    "OPENSTACK": {
      "module": "escape.adapt.managers",
      "class": "UnifyDomainManager",
      "domain_name": "OPENSTACK",
      "diff": true,
      "poll": false,
      "adapters": {
        "REMOTE": {
          "module": "escape.adapt.adapters",
          "class": "UnifyRESTAdapter",
          "prefix": "ro/os",
          "url": "http://127.0.0.1:8899",
          "timeout": 1
        }
      }
    },
    "DOCKER-1": {
      "module": "escape.adapt.managers",
      "class": "UnifyDomainManager",
      "domain_name": "DOCKER-1",
      "diff": true,
      "poll": false,
      "adapters": {
        "REMOTE": {
          "module": "escape.adapt.adapters",
          "class": "UnifyRESTAdapter",
          "prefix": "ro/docker1",
          "url": "http://127.0.0.1:8899"
        }
      }
    },
    "UN": {
      "module": "escape.adapt.managers",
      "class": "UnifyDomainManager",
      "domain_name": "UN",
      "diff": true,
      "poll": false,
      "adapters": {
        "REMOTE": {
          "module": "escape.adapt.adapters",
          "class": "UnifyRESTAdapter",
          "prefix": "ro/un",
          "url": "http://127.0.0.1:8899"
        }
      }
    },
    "RYU": {
      "module": "escape.adapt.managers",
      "class": "UnifyDomainManager",
      "domain_name": "RYU",
      "diff": true,
      "poll": false,
      "adapters": {
        "REMOTE": {
          "module": "escape.adapt.adapters",
          "class": "UnifyRESTAdapter",
          "prefix": "ro/ryu",
          "url": "http://127.0.0.1:8899"
        }
      }
    }
  },
  "infrastructure": {},
  "visualization": {
    "url": "http://192.168.1.206:2222",
    "rpc": "/notification"
  }
}
