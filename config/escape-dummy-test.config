      "OPENSTACK",
      "DOCKER-1",
      "UN",
      "RYU"
    ],
    "MININET": {
      "module": "escape.adapt.managers",
      "class": "BasicDomainManager",
      "domain_name": "MININET",
      "adapters": {
        "TOPOLOGY": {
          "module": "escape.adapt.adapters",
          "class": "VirtualizerBasedStaticFileAdapter",
          "path": "dummy-orchestrator/xmls/00-mininet.xml",
          "log_dir": "log/",
          "diff": true
        }
